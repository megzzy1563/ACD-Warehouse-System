{
  "app": {
    "name": "ACD Inventory API",
    "apiVersion": "1.0.0"
  },
  "cors": {
    "origins": ["http://localhost:3000"],
    "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
    "allowedHeaders": ["Content-Type", "Authorization"]
  },
  "jwt": {
    "expireIn": "30d"
  },
  "logging": {
    "level": "info"
  },
  "pagination": {
    "defaultLimit": 10,
    "maxLimit": 100
  },
  "uploads": {
    "maxSize": 5000000,
    "allowedTypes": ["image/jpeg", "image/png", "image/gif"]
  },
  "inventory": {
    "components": ["Engine", "Hydraulic", "Electrical", "Mechanical", "Body"],
    "rackLocations": ["A1", "A2", "B1", "B2", "C1"]
  }
}